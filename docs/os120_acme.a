    CLC                                                 ;
    ADC .channel0EnvelopeOffset - .bufferNumberSound0,X ; add it to envelope offset
    TAY                                                 ; transfer to Y
    LDA .envelopeBuffer + 11,Y                          ; and get target value at end of current phase
    SEC                                                 ;
    SBC #$3F                                            ;
